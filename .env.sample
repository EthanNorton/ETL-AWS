# AWS Credentials
AWS_ACCESS_KEY_ID=your_access_key_id
AWS_SECRET_ACCESS_KEY=your_secret_access_key
AWS_REGION=your_aws_region

# Example for DynamoDB Table Name
DYNAMODB_TABLE=my_dynamodb_table

# Example for S3 Bucket Name
S3_BUCKET=my_s3_bucket

# Example for SQS Queue URL
SQS_QUEUE_URL=https://sqs.region.amazonaws.com/account-id/queue-name

# Example for SNS Topic ARN
SNS_TOPIC_ARN=arn:aws:sns:region:account-id:topic-name

# Example for API Gateway Base URL
API_BASE_URL=https://api.gateway.url

# Other configuration variables
ENVIRONMENT=development
DEBUG=True

